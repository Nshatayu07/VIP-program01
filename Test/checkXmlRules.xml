                <param name="expression" value="forth" />
                <param name="expression" value="third" />
            </flipstrategy>
            <custom-properties>
                <property name="ppint" value="12"/>
                <property name="ppint" value="12"/>
            </custom-properties>
    </feature>
    <feature uid="ff4j.consoleAdmmn"
        enable="false"
        description="Limit access to administration console (AuthorizationManager)">
            <flipstrategy class="org.ff4j.strategy.el.ExpressionFlipStrategy">
                <param name="expression" value="first" />
                <param name="expression" value="second" />  
            </flipstrategy>
            <custom-properties>
                <jiraid name="pascal"/>
                <property name="ppint" value="52"/>
                <property name="ppint" value="52"/>
            </custom-properties> 
    </feature> 
</features>
